CREATE TABLE Variation_data (
  SNP VARCHAR(12),
  Location VARCHAR(35),
  Allele VARCHAR(20),
  Consequence VARCHAR(100),
  IMPACT VARCHAR(20),
  SYMBOL VARCHAR(35),
  Gene VARCHAR(20),
  Feature_type VARCHAR(35),
  Feature VARCHAR(35),
  BIOTYPE VARCHAR(35),
  EXON VARCHAR(35),
  INTRON VARCHAR(35),
  cDNA_position INT,
  CDS_position INT,
  Protein_position VARCHAR(35),
  Amino_acids VARCHAR(35),
  Codons VARCHAR(35),
  DISTANCE VARCHAR(35),
  STRAND VARCHAR(35),
  FLAGS VARCHAR(35),
  SYMBOL_SOURCE VARCHAR(35),
  HGNC_ID VARCHAR(35),
  MANE_SELECT VARCHAR(35),
  MANE_PLUS_CLINICAL VARCHAR(35),
  TSL INT,
  APPRIS VARCHAR(35),
  AF REAL,
  CLIN_SIG VARCHAR(35),
  LoFtool REAL,
  CADD_PHRED REAL,
  CADD_RAW VARCHAR(10),
  PRIMARY KEY (SNP)
);

.mode csv
.import '/home/bt211031/Downloads/speadsheet of full diabetes funtional data up to date (copy).csv' Variation_data

